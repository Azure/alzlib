{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach (dlv exec <cmd> --headless --listen=127.0.0.1:40000 --api-version=2 --)",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "host": "127.0.0.1",
      "port": 40000,
      "showLog": true,
      "substitutePath": [
        {
          "from": "github.com/Azure/alzlib",
          "to": "${workspaceFolder}"
        }
      ]
    }
  ]
}
